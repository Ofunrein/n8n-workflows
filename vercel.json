{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python",
      "includeFiles": "workflows/**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "WORKFLOW_DB_PATH": "/tmp/workflows.db",
    "PYTHONPATH": "."
  }
}
