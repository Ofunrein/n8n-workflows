{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "WORKFLOW_DB_PATH": "/tmp/workflows.db",
    "PYTHONPATH": "."
  },
  "functions": {
    "vercel_app.py": {
      "maxDuration": 10
    }
  }
}
